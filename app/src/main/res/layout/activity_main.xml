<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <EditText
        android:layout_width="373dp"
        android:layout_height="72dp"
        android:inputType="textPersonName"
        android:hint="Enter Android Version"
        android:ems="10"

        android:layout_alignParentStart="true" android:layout_marginStart="23dp"
        android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
        android:id="@+id/et1"
        android:layout_marginTop="6dp" android:layout_marginLeft="23dp"/>
    <EditText
        android:layout_width="371dp"
        android:layout_height="69dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:hint="Enter Android Code Name"
        android:layout_alignParentStart="true" android:layout_marginStart="23dp"
        android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
        android:id="@+id/et2"
        android:layout_marginTop="90dp" android:layout_marginLeft="23dp"/>
    <Button
        android:text="ADD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:layout_alignParentStart="true"
        android:layout_marginStart="0dp" android:layout_alignParentLeft="true"
        android:onClick="add"
        android:layout_alignParentTop="true" android:id="@+id/button" android:layout_marginTop="173dp"
        android:layout_marginLeft="0dp"/>

    <!-- Table layout-->
    <TableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="483dp" android:layout_alignParentStart="true"
        android:layout_marginStart="0dp" android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="248dp"
        android:layout_marginLeft="0dp"
        android:stretchColumns="*"
        android:padding="2dp"
        android:id="@+id/tableviews">
        <!-- Row 1-->
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#000"
            android:layout_marginBottom="3dp">

            <TextView

                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="Version"
                android:textColor="#000"
                android:layout_marginRight="1dp"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"

                android:textSize="14dp"

                />
            <TextView

                android:id="@+id/tv2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="Code Name"
                android:textColor="#000"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                android:textSize="14dp" />
        </TableRow>



        <!-- Row 2-->
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="3dp"
            android:background="#000">
            <TextView

                android:id="@+id/tv3"
                android:layout_marginRight="1dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="Android 9.0"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"

                android:textColor="#000"

                android:textSize="14dp"

                />
            <TextView

                android:id="@+id/tv4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="Pie"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"

                android:textColor="#000"
                android:textSize="14dp" />
        </TableRow>

        <!-- Row 3-->

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="3dp"
            android:background="#000">
            <TextView

                android:id="@+id/tv5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="android 8.0"
                android:textColor="#000"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"
                android:textSize="14dp"


                />
            <TextView

                android:id="@+id/tv6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#fd3d7e"
                android:text="Oreo"
                android:textColor="#000"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"

                android:textSize="14dp" />
        </TableRow>



    </TableLayout>


</RelativeLayout>